{
    "name": "inisiatif/enqueue",
    "description": "Enqueue package using in Inisiatif Zakat Indonesia",
    "license": "proprietary",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "enqueue/amqp-lib": "^0.10",
        "enqueue/enqueue": "^0.10",
        "enqueue/null": "^0.10",
        "enqueue/simple-client": "^0.10",
        "php-amqplib/php-amqplib": "^3.7"
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.33"
    },
    "autoload": {
        "psr-4": {
            "Inisiatif\\Package\\Enqueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inisiatif\\Package\\Enqueue\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
